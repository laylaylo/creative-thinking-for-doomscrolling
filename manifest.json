{
  "manifest_version": 3,
  "name": "InstaFocus Quiz",
  "version": "1.0",
  "description": "Blocks Instagram after 10 minutes until a question is answered.",
  "permissions": ["storage"],
  "host_permissions": ["*://*.instagram.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}